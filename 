[1mdiff --cc src/components/FileBase64.jsx[m
[1mindex 0000000,0000000..a9e0e98[m
[1mnew file mode 100644[m
[1m--- /dev/null[m
[1m+++ b/src/components/FileBase64.jsx[m
[36m@@@ -1,0 -1,0 +1,78 @@@[m
[32m++/*! Copyright (c) 2016 Naufal Rabbani (http://github.com/BosNaufal)[m
[32m++* Licensed Under MIT (http://opensource.org/licenses/MIT)[m
[32m++*[m
[32m++* React File Base64 - Version@1.0.0[m
[32m++*[m
[32m++*/[m
[32m++[m
[32m++import React from 'react';[m
[32m++[m
[32m++export default class FileBase64 extends React.Component {[m
[32m++[m
[32m++  constructor(props) {[m
[32m++    super(props);[m
[32m++    this.state = {[m
[32m++      files: [],[m
[32m++    };[m
[32m++  }[m
[32m++[m
[32m++  handleChange(e) {[m
[32m++[m
[32m++    // get the files[m
[32m++    let files = e.target.files;[m
[32m++[m
[32m++    // Process each file[m
[32m++    var allFiles = [];[m
[32m++    for (var i = 0; i < files.length; i++) {[m
[32m++[m
[32m++      let file = files[i];[m
[32m++[m
[32m++      // Make new FileReader[m
[32m++      let reader = new FileReader();[m
[32m++[m
[32m++      // Convert the file to base64 text[m
[32m++      reader.readAsDataURL(file);[m
[32m++[m
[32m++      // on reader load somthing...[m
[32m++      reader.onload = () => {[m
[32m++[m
[32m++        // Make a fileInfo Object[m
[32m++        let fileInfo = {[m
[32m++          name: file.name,[m
[32m++          type: file.type,[m
[32m++          size: Math.round(file.size / 1000) + ' kB',[m
[32m++          base64: reader.result,[m
[32m++          file: file,[m
[32m++        };[m
[32m++[m
[32m++        // Push it to the state[m
[32m++        allFiles.push(fileInfo);[m
[32m++[m
[32m++        // If all files have been proceed[m
[32m++        if(allFiles.length === files.length){[m
[32m++          // Apply Callback function[m
[32m++          if(this.props.multiple) this.props.onDone(allFiles);[m
[32m++          else this.props.onDone(allFiles[0]);[m
[32m++        }[m
[32m++[m
[32m++      } // reader.onload[m
[32m++[m
[32m++    } // for[m
[32m++[m
[32m++  }[m
[32m++[m
[32m++  render() {[m
[32m++    return ([m
[32m++      <input[m
[32m++        type="file"[m
[32m++        onChange={ this.handleChange.bind(this) }[m
[32m++        multiple={ this.props.multiple }[m
[32m++        accept="image/*"[m
[32m++        />[m
[32m++    );[m
[32m++  }[m
[32m++}[m
[32m++[m
[32m++FileBase64.defaultProps = {[m
[32m++  multiple: false,[m
[32m++};[m
